          "usingPsuInit": {
            "runPsuInit": true,
            "plPowerup": false,
            "psuInitTclFile": "${workspaceFolder}\\pmufw\\_ide\\psinit\\psu_init.tcl"
          }
        },
        "downloadElf": [
          {
            "core": "psu_pmu_0",
            "resetProcessor": true,
            "elfFile": "${workspaceFolder}\\pmufw\\build\\pmufw.elf",
            "stopAtEntry": false
          }
        ],
        "crossTriggerBreakpoints": {
          "isSelected": false,
          "breakpoints": []
